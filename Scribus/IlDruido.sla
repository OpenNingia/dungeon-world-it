            <ITEXT FONTSIZE="8" KERN="-5" CH="Scegli un "/>
            <ITEXT FONT="DejaVu Serif Condensed Italic" FONTSIZE="8" KERN="-5" CH="Marchio"/>
            <ITEXT FONTSIZE="8" KERN="-5" CH=". Questa è una caratteristica fisica legata alla terreno che hai scelto, e che non è persa neanche quando cambi forma."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONT="DejaVu Serif Condensed Bold" FONTSIZE="8" KERN="-5" CH="SOSTENUTO DALLA NATURA"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONTSIZE="8" KERN="-5" CH="Non hai bisogno di mangiare e bere. Se una mossa dice di consumare una razione, non farlo semplicemente."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONT="DejaVu Serif Condensed Bold" FONTSIZE="8" KERN="-5" CH="LINGUA DEGLI SPIRITI"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONTSIZE="8" KERN="-5" CH="Puoi parlare a un animale tipico del tuo terreno, o a uno simile o accomunato in qualche modo, come se parlassi a una persona."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONT="DejaVu Serif Condensed Bold" FONTSIZE="8" KERN="-5" CH="MUTAFORMA"/>
